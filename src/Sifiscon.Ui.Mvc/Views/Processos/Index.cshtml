@model IEnumerable<ProcessoViewModel>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Cnpj)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumeroProcesso)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RelatoFiscalizacao)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataRelato)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FiscalResposavel)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cnpj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumeroProcesso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RelatoFiscalizacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataRelato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FiscalResposavel)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
